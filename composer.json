{
    "name": "radiocubito/laravel-matryoshka",
    "description": "Russian Doll Caching for Laravel.",
    "keywords": [
        "radiocubito",
        "matryoshka"
    ],
    "homepage": "https://github.com/radiocubito/laravel-matryoshka",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dario Oliver",
            "email": "oliver@radiocubito.com",
            "homepage": "https://radiocubito.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "5.8.*",
        "illuminate/database": "5.8.*",
        "illuminate/cache": "5.8.*",
        "illuminate/filesystem": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Radiocubito\\Matryoshka\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radiocubito\\Matryoshka\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Radiocubito\\Matryoshka\\MatryoshkaServiceProvider"
            ]
        }
    }
}
